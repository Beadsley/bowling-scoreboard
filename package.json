{
  "name": "bowling-game-kata",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "npx eslint -c .eslintrc.json ./**/*.js",
    "test": "mocha --reporter spec --exit \"./{,!(node_modules)/**/}*.spec.js\"",
    "watch": "onchange '**/*.js' -- npm run test",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "heroku-postbuild": "npm run install-client && npm run build",
    "start": "node index.js",
    "client": "cd client && npm start",
    "dev": "nodemon index.js",
    "everything": "concurrently -n 'sever,client' -c 'red,green' \"npm run dev\" \"npm run client\""
  },
  "keywords": [],
  "license": "MIT",
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-mocha": "^6.2.1",
    "mocha": "^6.2.2",
    "morgan": "^1.9.1",
    "nodemon": "^2.0.2",
    "onchange": "^6.1.0"
  },
  "dependencies": {
    "@material-ui/lab": "^4.0.0-alpha.56",
    "body-parser": "^1.19.0",
    "client": "file:client",
    "concurrently": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-http-context": "^1.2.3",
    "mongoose": "^5.9.5",
    "uuid": "^7.0.1"
  }
}
